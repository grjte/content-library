                        "type": "string"
                    }
                },
                "director": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "genre": {
                    "type": "string"
                },
                "imdbId": {
                    "type": "string"
                },
                "writer": {
                    "type": "array",
